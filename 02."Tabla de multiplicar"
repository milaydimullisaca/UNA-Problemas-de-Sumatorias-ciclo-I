#include <iostream>
using namespace std;
void imprimirTabla(int n)
{

    cout << "Tabla de multiplicar del "<<n<<endl;
    for (int i = 1; i <= 10; i++) {
        cout<<n<<"x"<<i<<"="<<n*i<<endl;
        
    }
    cout<<endl;
}
int main(){
    for(int i =1;i<=10;i++){

        imprimirTabla(i);
    }
    return 0;
}
